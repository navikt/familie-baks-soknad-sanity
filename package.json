{
  "name": "baks-soknad-cms",
  "private": true,
  "version": "1.0.0",
  "description": "Sanity (cms) for søknad til kontantstøtte",
  "main": "package.json",
  "author": "Daphne <daphne.leebeek@nav.no>",
  "scripts": {
    "dev": "sanity dev",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "lint": "eslint-check && prettier-check",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "eslint-check": "eslint .",
    "prettier-check": "prettier --check ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix ./"
    ]
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/client": "^6.15.0",
    "@sanity/vision": "^3.37.2",
    "groq": "^3.37.2",
    "react": "^18.x",
    "react-dom": "^18.x",
    "sanity": "3.37.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@sanity/eslint-config-studio": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-app": "^6.2.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
