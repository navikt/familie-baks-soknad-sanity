{
  "name": "baks-soknad-cms",
  "private": true,
  "version": "1.0.0",
  "description": "Sanity (cms) for søknad til kontantstøtte",
  "main": "package.json",
  "author": "Team BAKS",
  "scripts": {
    "dev": "sanity dev",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "lint": "yarn eslint-check && yarn prettier-check",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "eslint-check": "eslint .",
    "prettier-check": "prettier --check ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix ./"
    ]
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/client": "7.14.0",
    "@sanity/vision": "5.2.0",
    "groq": "5.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sanity": "5.2.0",
    "styled-components": "6.3.5"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@sanity/eslint-config-studio": "6.0.0",
    "typescript-eslint": "8.53.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "globals": "17.3.0",
    "prettier": "3.8.1",
    "typescript": "5.9.2"
  }
}
